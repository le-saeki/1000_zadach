#include <iostream>
using namespace std;

void bubble(int*arr, int size)
{
	for(int i=0;i<size;++i)
	{
		for(int j=0;j<size-1-i;++j)
		{
			if(arr[j]<arr[j+1])
			{
				swap(arr[j],arr[j+1]);
			}
		
		}
	}
}

void f(int*arr, int size)
{
	for(int k=0;k<size;++k)
	{cout<<arr[k]<< " ";
	}
}

int main() {
	int array[5] {1,4,8,5,9};
	bubble(array,5);
   f(array,5);
	
	return 0;
}
